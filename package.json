{
  "name": "VEngine",
  "version": "1.0.0",
  "description": "Editor for VEngine",
  "main": "app.js",
  "scripts": {
    "start": "./node_modules/.bin/electron .",
    "package-all": "electron-packager . VEngine --overwrite --asar --platform=all --arch=all --icon=ResourcesStatic/icons/iconT.ico --prune=true --out=builds --ignore=.bat --ignore=.gitattributes --ignore=.gitignore --ignore=LICENSE --ignore=config.json --ignore=default_layout.json --ignore=.vscode --ignore=Libraries --ignore=Modules --ignore=Plugins --ignore=ResourcesDynamic",
    "package-win32": "electron-packager . VEngine --overwrite --asar --platform=win32 --arch=ia32 --icon=ResourcesStatic/icons/iconT.ico --prune=true --out=builds --ignore=.bat --ignore=.gitattributes --ignore=.gitignore --ignore=LICENSE --ignore=config.json --ignore=default_layout.json --ignore=.vscode --ignore=Libraries --ignore=Modules --ignore=Plugins --ignore=ResourcesDynamic",
    "package-win64": "electron-packager . VEngine --overwrite --asar --platform=win32 --arch=x64 --icon=ResourcesStatic/icons/iconT.ico --prune=true --out=builds --ignore=.bat --ignore=.gitattributes --ignore=.gitignore --ignore=LICENSE --ignore=config.json --ignore=default_layout.json --ignore=.vscode --ignore=Libraries --ignore=Modules --ignore=Plugins --ignore=ResourcesDynamic",
    "package-mac": "electron-packager . VEngine --overwrite --asar --platform=darwin --arch=x64 --icon=ResourcesStatic/icons/iconT.ico --prune=true --out=builds --ignore=.bat --ignore=.gitattributes --ignore=.gitignore --ignore=LICENSE --ignore=config.json --ignore=default_layout.json --ignore=.vscode --ignore=Libraries --ignore=Modules --ignore=Plugins --ignore=ResourcesDynamic",
    "package-linux": "electron-packager . VEngine --overwrite --asar --platform=linux --arch=x64 --icon=ResourcesStatic/icons/iconT.ico --prune=true --out=builds --ignore=.bat --ignore=.gitattributes --ignore=.gitignore --ignore=LICENSE --ignore=config.json --ignore=default_layout.json --ignore=.vscode --ignore=Libraries --ignore=Modules --ignore=Plugins --ignore=ResourcesDynamic"
  },
  "author": "wiktortr",
  "license": "GNU",
  "dependencies": {
    "crypto": "^1.0.1",
    "deasync": "^0.1.12",
    "electron": "^1.8.2",
    "electron-debug": "^1.5.0",
    "jquery": "^3.3.1",
    "system-sleep": "^1.3.6",
    "three.js": "^0.77.1",
    "watchjs": "0.0.0"
  }
}
